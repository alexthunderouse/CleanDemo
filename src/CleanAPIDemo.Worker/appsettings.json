{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=CleanAPIDemo;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Graylog": {
    "Host": "localhost",
    "Port": 12201,
    "Facility": "CleanAPIDemo.Worker"
  },
  "DataSyncJob": {
    "IntervalMinutes": 10,
    "ApiKey": "",
    "SourceEndpoint": "https://api.example.com/data",
    "MaxRetryAttempts": 3,
    "RetryDelaySeconds": 2
  },
  "HealthCheck": {
    "Port": 5050
  }
}
